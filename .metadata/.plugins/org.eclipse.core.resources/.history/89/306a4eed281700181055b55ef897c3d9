import static java.lang.System.*;

public class Triples
{
   private int number;

	public Triples()
	{
		this(0);
	}

	public Triples(int num)
	{
		setNum(num);

	}

	public void setNum(int num)
	{
		number = num;

	}
	
	private int greatestCommonFactor(int a, int b, int c)
	{
		int max = 0;
		
		if (a > b && a > c){
			for (int i = 1; i < a; i++){
				if (a%i==0 && b%i == 0 && c%i ==0){
					max = i;
				}
			}
		}
		
		if (b>a && b>c) {
			for (int i = 1; i < b; i++){
				if (a%i == 0 && b%i == 0 && c%i ==0){
					max = i;
				}
			}
		}
		
		else {
			for (int i = 1; i < c; i++){
				if (a%i == 0 && b%i == 0 && c%i ==0){
					max = i;
				}
			}
		}
		return max;
	}

	public String toString()
	{
		String output="";






		return "the max is: " + greatestCommonFactor(6, 18, 36) +"\n";
	}
}