import java.util.Scanner;
import static java.lang.System.*;

public class FancyWord
{
	private char[][] mat;

	public FancyWord()
	{

	}

   public FancyWord(String s)
	{
	   int row = s.length();
	   mat = new char[s.length()][s.length()];
	  
	   for (int i = 0; i < s.length(); i++){
		   mat[row][i] = s.charAt(i);
	   }
	   
		   
	   
	   

	}

	public String toString()
	{
		String output="";
		
		output += mat;




		return output+"\n\n";
	}
}