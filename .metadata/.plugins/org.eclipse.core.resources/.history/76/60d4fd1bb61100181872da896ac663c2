import static java.lang.System.*;
import java.lang.Math;

public class Prime
{
	private int number;

	public Prime()
	{


	}

	public Prime(int num)
	{
		setPrime(num);

	}

	public void setPrime(int num)
	{
		number = num;

	}

	public boolean isPrime()
	{
		boolean testPrime = true;
		for (int i = 1; i < number +1; i++){
			if (number % i != 0){
				testPrime = true;
			}
			else {
				testPrime = false;
			}
		}
		
		if (testPrime == true){
			return true;
		}
		else {
			return false;
		}
	}

	public String toString()
	{
		String output = "";
		if (isPrime() == true){
			output= "not a prime number";
		}
		else {
			output = "is a prime number";
		}
		







		return output;
	}
}