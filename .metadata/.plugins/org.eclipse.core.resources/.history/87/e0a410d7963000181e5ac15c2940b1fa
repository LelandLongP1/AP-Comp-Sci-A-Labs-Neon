import java.util.Scanner;
import static java.lang.System.*;

public class TicTacToe
{
	private char[][] mat;

	public TicTacToe()
	{


	}

	public TicTacToe(String game)
	{
		mat = new char[3][3];
		int j = 0;
		for (int i = 0; i < 3; i++){	
			for (int k = 0; k < 3; k++){
				mat[i][k] = game.charAt(j);
				j++;
			}
		}
		for (int i = 0; i < 3; i++){
			for (int k = 0; k < 3; k++){
				out.print(mat[i][k]);
			}
			out.println();
		}


	}

	public String getWinner()
	{
		//horizontal 
		for (int i = 0; i < 2; i++){	
			int k = 0;
			char temp = mat[i][k];
			if (mat[i][k+1] == temp){
				if (mat[i][k+2] == temp){
					return temp + "wins horizontally";
				}
			}
		}











		return "";
	}

	public String toString()
	{
		String output="";
		output += getWinner();






		return output+"\n\n";
	}
}